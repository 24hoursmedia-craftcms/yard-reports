{
    "name": "twentyfourhoursmedia/yard-reports",
    "description": "Export sprout forms to CSV (Excel supported)",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "yard reports",
        "sprout forms"
    ],
    "support": {
        "docs": "https://io.24hoursmedia.com/yard-reports",
        "issues": "https://github.com/24hoursmedia-craftcms/yard-reports/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "24HOURSMEDIA",
            "homepage": "https://en.24hoursmedia.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "craftcms/cms": "^3.0.0"
