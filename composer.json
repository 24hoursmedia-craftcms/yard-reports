    "autoload": {
        "psr-4": {
          "twentyfourhoursmedia\\yardreports\\": "src/"
        }
    },
    "extra": {
        "name": "Yard Reports",
        "handle": "yard-reports",
        "developer": "24HOURSMEDIA",
        "developerUrl": "https://en.24hoursmedia.com",
        "documentationUrl": "https://github.com/24hoursmedia-craftcms/yard-reports/yard-reports/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/24hoursmedia-craftcms/yard-reports/yard-reports/master/CHANGELOG.md",
        "components": {
            "yardReportsService": "twentyfourhoursmedia\\yardreports\\services\\YardReportsService"
        },
        "class": "twentyfourhoursmedia\\yardreports\\YardReports"
    }
